{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "apps": {
    "ios": {
      "type": "ios.app",
      "build": "xcodebuild -workspace ./ios/VehicleTracker.xcworkspace -configuration Debug -scheme VehicleTracker -destination id=1FB77DE8-ADA6-4837-99DD-FDFD3E3F7F4F -sdk iphonesimulator -derivedDataPath ios/build",
      "binaryPath": "./ios/build/Build/Products/Debug-iphonesimulator/VehicleTracker.app"
    },
    "android": {
      "type": "android.apk",
      "binaryPath": "SPECIFY_PATH_TO_YOUR_APP_BINARY"
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 11"
      }
    },
    "emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "Pixel_3a_API_30_x86"
      }
    }
  },
  "configurations": {
    "ios": {
      "device": "simulator",
      "app": "ios"
    },
    "android": {
      "device": "emulator",
      "app": "android"
    }
  }
}